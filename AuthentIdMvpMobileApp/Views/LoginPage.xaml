<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AuthentIdMvpMobileApp.Views.LoginPage"
             Title="LoginPage">
    <VerticalStackLayout>
        <Entry Placeholder="Username" TextColor="Black"/>
        <Entry Placeholder="Password" TextColor="Black" IsPassword="True"/>
        <Button Text="Login" BackgroundColor="Black" TextColor="White"/>
        <!--<Button Text="Login" BackgroundColor="Black" TextColor="White" Command="{Binding LoginCommand}"/>-->
    </VerticalStackLayout>
</ContentPage>